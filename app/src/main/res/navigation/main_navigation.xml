<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/main_navigation"
    app:startDestination="@id/mainMenuFragment">

    <fragment
        android:id="@+id/mainMenuFragment"
        android:name="com.sparktech.studyzoneadmin.main_menu.MainMenuFragment"
        android:label="MainMenuFragment" >
        <action
            android:id="@+id/action_mainMenuFragment_to_schoolFragment"
            app:destination="@id/schoolFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_transactionsFragment"
            app:destination="@id/transactionsFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_userFragment"
            app:destination="@id/userFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_withdrawalRequestFragment"
            app:destination="@id/withdrawalRequestFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_chatListFragment"
            app:destination="@id/chatListFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_annoucementFragment"
            app:destination="@id/annoucementFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_vendorFragment"
            app:destination="@id/vendorFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_lisenseKeyFragment"
            app:destination="@id/lisenseKeyFragment" />
    </fragment>
    <fragment
        android:id="@+id/schoolFragment"
        android:name="com.sparktech.studyzoneadmin.school.SchoolFragment"
        android:label="SchoolFragment" >
        <action
            android:id="@+id/action_schoolFragment_to_schoolDetails"
            app:destination="@id/schoolDetails" />
    </fragment>
    <fragment
        android:id="@+id/schoolDetails"
        android:name="com.sparktech.studyzoneadmin.school.SchoolDetails"
        android:label="SchoolDetails" >
        <action
            android:id="@+id/action_schoolDetails_to_schoolDepartmentFragment"
            app:destination="@id/schoolDepartmentFragment" />
    </fragment>
    <fragment
        android:id="@+id/schoolDepartmentFragment"
        android:name="com.sparktech.studyzoneadmin.school.SchoolDepartmentFragment"
        android:label="SchoolDepartmentFragment" >
        <action
            android:id="@+id/action_schoolDepartmentFragment_to_schoolLevelFragment"
            app:destination="@id/schoolLevelFragment" />
    </fragment>
    <fragment
        android:id="@+id/schoolLevelFragment"
        android:name="com.sparktech.studyzoneadmin.school.SchoolLevelFragment"
        android:label="SchoolLevelFragment" >
        <action
            android:id="@+id/action_schoolLevelFragment_to_pastQuestionFragment"
            app:destination="@id/pastQuestionFragment" />
    </fragment>
    <fragment
        android:id="@+id/pastQuestionFragment"
        android:name="com.sparktech.studyzoneadmin.past_question.PastQuestionFragment"
        android:label="PastQuestionFragment" />
    <fragment
        android:id="@+id/transactionsFragment"
        android:name="com.sparktech.studyzoneadmin.transaction.TransactionsFragment"
        android:label="TransactionsFragment" />
    <fragment
        android:id="@+id/userFragment"
        android:name="com.sparktech.studyzoneadmin.user.UserFragment"
        android:label="UserFragment" >
        <action
            android:id="@+id/action_userFragment_to_userDetails"
            app:destination="@id/userDetails" />
    </fragment>
    <fragment
        android:id="@+id/userDetails"
        android:name="com.sparktech.studyzoneadmin.user.UserDetails"
        android:label="UserDetails" />
    <fragment
        android:id="@+id/withdrawalRequestFragment"
        android:name="com.sparktech.studyzoneadmin.withdrawal_request.WithdrawalRequestFragment"
        android:label="WithdrawalRequestFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.sparktech.studyzoneadmin.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/chatListFragment"
        android:name="com.sparktech.studyzoneadmin.live_support.ChatListFragment"
        android:label="ChatListFragment" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.sparktech.studyzoneadmin.notification.NotificationFragment"
        android:label="NotificationFragment" />
    <fragment
        android:id="@+id/annoucementFragment"
        android:name="com.sparktech.studyzoneadmin.annnouncement.AnnoucementFragment"
        android:label="AnnoucementFragment" />
    <fragment
        android:id="@+id/lisenseKeyFragment"
        android:name="com.sparktech.studyzoneadmin.lisense_key.LisenseKeyFragment"
        android:label="LisenseKeyFragment" />
    <fragment
        android:id="@+id/vendorFragment"
        android:name="com.sparktech.studyzoneadmin.vendor.VendorFragment"
        android:label="VendorFragment" >
        <action
            android:id="@+id/action_vendorFragment_to_vendorStatisticsFragment"
            app:destination="@id/vendorStatisticsFragment" />
    </fragment>
    <fragment
        android:id="@+id/vendorStatisticsFragment"
        android:name="com.sparktech.studyzoneadmin.vendor.VendorStatisticsFragment"
        android:label="VendorStatisticsFragment" />
</navigation>